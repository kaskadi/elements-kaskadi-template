{
  "name": "elements-kaskadi-template",
  "version": "1.0.0",
  "description": "",
  "main": "kaskadi-template.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "example": "xdg-open localhost:1234/example && serve -l 1234",
    "test": "standard --fix && karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "kaskadi-init": "kaskadi init && git add -u && git commit -m 'initialized from template'",
    "coverage":"cc-test-reporter before-build && npm test && cc-test-reporter after-build -t lcov -r 7b14f1f9497bb8e48bbd7efbe19c985f33ddc452ac0a5931699136a43494c6ee"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaskadi/elements-kaskadi-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaskadi/elements-kaskadi-template/issues"
  },
  "homepage": "https://github.com/kaskadi/elements-kaskadi-template#readme",
  "kaskadi": {
    "s3-push": {
      "files": [
        {
          "src": "kaskadi-template.js",
          "dest": "modules/@kaskadi/kaskadi-template/{branch}kaskadi-template.js"
        },
        {
          "src": "example/index.html",
          "dest": "modules/@kaskadi/kaskadi-template/{branch}example/index.html"
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.0.0",
    "standard": "^14.3.1"
  }
}
